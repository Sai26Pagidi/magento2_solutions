<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
       <item name="js_config" xsi:type="array">
           <item name="provider" xsi:type="string">job_listing_editForm.job_listing_editForm_data_source</item></item>
           <item name="label" xsi:type="string" translate="true">General Information</item>
           <!-- <item name="template" xsi:type="string">templates/form/collapsible</item> -->
           <!-- if we uncomment line no-7 the page will load continuously after adding tabs section layout to display tabs we need add admin-2colums-left in blogadmin_create_edit.xml -->
           </argument>
              <settings>
                 <!-- <buttons>
                    <button name="back" class="Thecoachsmb\Grid\Block\Adminhtml\Customform\Edit\BackButton"/>
                    <button name="delete" class="Thecoachsmb\Grid\Block\Adminhtml\Customform\Edit\DeleteButton"/>
                    <button name="reset" class="Thecoachsmb\Grid\Block\Adminhtml\Customform\Edit\ResetButton"/>
                    <button name="save" class="Thecoachsmb\Grid\Block\Adminhtml\Customform\Edit\SaveButton"/>
                    <button name="save_and_continue" class="Thecoachsmb\Grid\Block\Adminhtml\Customform\Edit\SaveAndContinueButton"/>
                 </buttons> -->
                 <buttons>
                    <button name="save" class="Teja\Blog\Block\Adminhtml\Create\Edit\SaveButton"/>
                 </buttons>
                 <!-- the functionality for this is added in line no-36 magento_ui -->
                 <!-- this is for sidebar menu/tabs by commenting line no-7 in this layout-->
                     <layout>
                         <navContainerName>left</navContainerName>
                         <type>tabs</type>
                   </layout>
                 <namespace>job_listing_editForm</namespace>
                 <dataScope>data</dataScope>
                 <deps>
                    <dep>job_listing_editForm.job_listing_editForm_data_source</dep>
                 </deps>
             </settings> 
             <dataSource name="job_listing_editForm_data_source">
               <argument name="data" xsi:type="array">
                  <item name="js_config" xsi:type="array">
                  <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
                  </item>
              </argument>
             <settings>
                <submitUrl path="*/*/save"/>
             </settings>
             <dataProvider name="job_listing_editForm_data_source" class="Teja\Blog\Model\Editform\DataProvider">
             <settings>
             <requestFieldName>id</requestFieldName>
             <primaryFieldName>job_id</primaryFieldName>
             </settings>
       </dataProvider>
       </dataSource>
       <!-- this must be unique name -->
       <fieldset name="general">
           <settings>
              <label>General</label>
              <!-- this is for dropdown collapsing true/false General section line no-7 helping us -->
              <collapsible>false</collapsible>
           </settings>
           <field name="job_name" formElement="input" sortOrder="10">
             <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                  <item name="source" xsi:type="string">Customform</item>
               </item>
             </argument>
             <settings>
               <dataType>text</dataType>
               <label translate="true">Job Name</label>
              <dataScope>job_name</dataScope>
              <validation>
                <rule name="required-entry" xsi:type="boolean">true</rule>
              </validation>
            </settings>
          </field>
          <field name="job_place" formElement="textarea" sortOrder="20">
              <argument name="data" xsi:type="array">
                 <item name="config" xsi:type="array">
                   <item name="source" xsi:type="string">Customform</item>
                 </item>
              </argument>
             <settings>
             <dataType>text</dataType>
             <label translate="true">Job Place</label>
             <dataScope>job_place</dataScope>
             <validation>
               <rule name="required-entry" xsi:type="boolean">true</rule>
             </validation>
            </settings>
         </field>
          <field name="created_at" formElement="date" sortOrder="30">
            <argument name="data" xsi:type="array">
                 <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">Customform</item>
                </item>
           </argument>
           <settings>
              <dataType>date</dataType>
              <label translate="true">Created At</label>
              <dataScope>created_at</dataScope>
               <validation>
                <rule name="required-entry" xsi:type="boolean">true</rule>
              </validation>
          </settings>
         </field> 
