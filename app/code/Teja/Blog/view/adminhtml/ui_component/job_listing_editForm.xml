           </settings>
           <field name="job_name" formElement="input" sortOrder="10">
             <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                  <item name="source" xsi:type="string">Customform</item>
               </item>
             </argument>
             <settings>
               <dataType>text</dataType>
               <label translate="true">Job Name</label>
              <dataScope>job_name</dataScope>
              <validation>
                <rule name="required-entry" xsi:type="boolean">true</rule>
              </validation>
            </settings>
          </field>
          <field name="job_place" formElement="textarea" sortOrder="20">
              <argument name="data" xsi:type="array">
                 <item name="config" xsi:type="array">
                   <item name="source" xsi:type="string">Customform</item>
                 </item>
              </argument>
             <settings>
             <dataType>text</dataType>
             <label translate="true">Job Place</label>
             <dataScope>job_place</dataScope>
             <validation>
               <rule name="required-entry" xsi:type="boolean">true</rule>
             </validation>
            </settings>
         </field>
          <field name="created_at" formElement="date" sortOrder="30">
            <argument name="data" xsi:type="array">
                 <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">Customform</item>
                </item>
           </argument>
           <settings>
              <dataType>date</dataType>
              <label translate="true">Created At</label>
              <dataScope>created_at</dataScope>
               <validation>
